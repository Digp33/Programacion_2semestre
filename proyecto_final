#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <malloc.h> 
struct Nodo
{
  int id;
  char nombreActividad[30]; 
  char descripcionActividad[30]; 
  int prioridad;
  int estadoAvtividad;
  char categoria[];
  char nota[30];
  struct Nodo *izq;
  struct Nodo *der;
};
struct ArbolBinario
{
  struct Nodo *raiz;
}; 
/*
void inicializa_arbol(struct ArbolBinario *arbol);
